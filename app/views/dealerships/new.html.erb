<%= form_with url:'/dealerships', method: :post, local: true do |form| %>
  <br>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <br>
  <%= form.label :is_open %>
  <%= form.check_box :is_open, {}, true, false %>
  <br>
  <%= form.label :max_car_capacity %>
  <%= form.number_field :max_car_capacity %>
  <br>
  <%= form.label :is_full %>
  <%= form.check_box :is_full, {}, true, false %>
  <br>
  <br>
  <%= form.submit "Create Dealership" %>
<% end %>
