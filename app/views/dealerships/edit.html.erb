<%= form_with url: "/dealerships/#{@dealership.id}", method: :patch, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :is_open %>
  <%= form.text_field :is_open %>
  <%= form.label :max_car_capacity %>
  <%= form.text_field :max_car_capacity %>
  <%= form.label :is_full %>
  <%= form.text_field :is_full %>
  <%= form.submit 'Update Dealership' %>
<% end %>