<%= form_with url: "/dealerships/#{@dealership.id}/employees", method: :post, local: true do |form| %>
  <br>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <br>
  <br>
  <%= form.label :on_vacation? %>
  <%= form.select :on_vacation, ['false', 'true']%>
  <br>
  <br>
  <%= form.label :cars_sold %>
  <%= form.number_field :cars_sold %>
  <br>
  <br>
  <%= form.hidden_field :dealership_id %>
  <%= form.submit "Create Employee"%>
<% end %>
