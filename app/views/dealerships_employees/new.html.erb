<%= form_with url: "/dealerships/#{Dealership.find(params[:id]).id}/employees", method: :post, local: true do |form| %>
  <br>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <br>
  <%= form.label :on_vacation %>
  <%= form.check_box :on_vacation, {}, true, false %>
  <br>
  <%= form.label :cars_sold %>
  <%= form.number_field :cars_sold %>
  <br>
  <br>
  <%= form.hidden_field :dealership_id %>
  <%= form.submit "Create Employee"%>
<% end %>
